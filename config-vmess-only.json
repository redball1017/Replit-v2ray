{
  "inbounds": [
    {
      "port": 443,
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "VMESS_UUID",
            "alterId": 0,
            "security": "VMESS_SECURITY"
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "wsSettings": {
          "path": "/vmess"
        }
      }
    }
  ],
    "outbounds": [
        {
            "protocol": "freedom",
            "tag": "direct",
            "settings": {}
        },
        {
            "protocol": "blackhole",
            "tag": "blocked",
            "settings": {}
        },
        {
            "protocol": "socks",
            "tag": "sockstor",
            "settings": {
                "servers": [
                    {
                        "address": "127.0.0.1",
                        "port": 9050
                    }
                ]
            }
        },
        {
            "protocol": "freedom",
            "tag": "twotag",
            "streamSettings": {
                "network": "domainsocket",
                "dsSettings": {
                    "path": "apath",
                    "abstract": true
                }
            }
        }
    ],
    
    "routing": {
        "rules": [
            {
                "type": "field",
                "inboundTag": [
                    "onetag"
                ],
                "outboundTag": "twotag"
            },
            {
                "type": "field",
                "outboundTag": "sockstor",
                "domain": [
                    "geosite:tor"
                ]
            },
            {
                "type": "field",
                "outboundTag": "blocked",
                "domain": [
                    "geosite:category-ads-all"
                ]
            }
        ]
    },
    "dns": {
        "servers": [
            "https://unfiltered.adguard-dns.com/dns-query",
            "https://dns.google/dns-query",
            "https://dns.cloudflare.com/dns-query"
        ]
    }
}
